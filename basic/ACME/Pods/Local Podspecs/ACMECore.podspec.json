{
  "name": "ACMECore",
  "version": "1.5",
  "summary": "The full suite needed to run any ACME app.",
  "homepage": "https://bitbucket.org/agens/acme",
  "authors": {
    "Agens": "dev@ivyengine.com"
  },
  "license": "Full copyright",
  "source": {
    "git": "git@bitbucket.org:agens/acme.git",
    "tag": "1.5"
  },
  "frameworks": [
    "SystemConfiguration",
    "IOKit",
    "CoreGraphics",
    "UIKit",
    "QuartzCore"
  ],
  "platforms": {
    "ios": "7.0"
  },
  "requires_arc": true,
  "source_files": [
    "Source/**/*.{h,m,mm,c}",
    "Static_Frameworks/**/*.{h,m,mm,c}"
  ],
  "exclude_files": [
    "Source/**/*Test.{h,m,mm,c}",
    "Static_Frameworks/**/*Test.{h,m,mm,c}"
  ],
  "ios": {
    "resource_bundles": {
      "ACMECore": [
        "Localization/**/*.lproj"
      ]
    }
  },
  "resources": [
    "Source/**/*.{xib}",
    "Resources/**/*.*",
    "Graphics/**/*.{jpg,jpeg,png,gif,sks}"
  ],
  "dependencies": {
    "AFNetworking": [
      "2.5.1"
    ],
    "AGGeometryKit": [
      "1.1"
    ],
    "ALSystemUtilities": [
      "1.3.4"
    ],
    "ARSafariActivity": [

    ],
    "BlocksKit/Core": [
      "2.2.5"
    ],
    "BlocksKit/UIKit": [
      "2.2.5"
    ],
    "CargoBay": [
      "2.1.0"
    ],
    "CocoaLumberjack": [
      "1.9.2"
    ],
    "EDColor": [
      "1.0.0"
    ],
    "FMDB": [
      "2.5"
    ],
    "FXKeychain": [
      "1.5.2"
    ],
    "FXReachability": [
      "1.3"
    ],
    "GBDeviceInfo": [
      "2.2.9"
    ],
    "HockeySDK": [
      "3.6.2"
    ],
    "iCarousel": [
      "1.8.1"
    ],
    "JLRoutes": [
      "1.5.1"
    ],
    "JSONModel": [
      "1.0.2"
    ],
    "NSLogger-CocoaLumberjack-connector": [
      "1.3"
    ],
    "pop": [
      "1.0.7"
    ],
    "ReactiveCocoa": [
      "2.4.7"
    ],
    "RNCryptor": [
      "2.2"
    ],
    "SSZipArchive": [

    ],
    "UISS": [
      "1.1.1"
    ]
  }
}
